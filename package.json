{
  "name": "vue-template",
  "version": "0.1.0",
  "private": true,
  "author": "shadow <https://www.github.com/stack-wuh>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "dev-test": "vue-cli-service build --mode test",
    "lint": "vue-cli-service lint"
  },
  "keywords": [
    "vue",
    "elementUI",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stack-wuh/vue-cli-template"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-plugin-component": "^1.1.1",
    "compression-webpack-plugin": "^2.0.0",
    "element-ui": "^2.4.11",
    "mockjs": "^1.0.1-beta3",
    "vue": "^2.5.21",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
